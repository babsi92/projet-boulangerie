{% extends 'base.html.twig' %}

{% block title %}Votre Panier
{% endblock %}

{% block body %}
	<style>
		.example-wrapper {
			margin: 1em auto;
			max-width: 800px;
			width: 95%;
			font: 18px / 1.5 sans-serif;
		}
		.example-wrapper code {
			background: #F5F5F5;
			ping: 2px 6px;
		}
	</style>

	<div class="example-wrapper">
		<ul>
			{% for produit in produits %}
				<li>{{produit.imageFile}}</li>
				<li>{{produit.nom}}
				</li>
			{% endfor %}
		</ul>

	</div>
{% endblock %}

